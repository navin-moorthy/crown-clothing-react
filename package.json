{
  "name": "crown-clothing",
  "version": "1.0.0",
  "engines": {
    "node": "10.16.0",
    "npm": "6.10.3"
  },
  "description": "Crown Clothing Ecommerce Site",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "stripe": "7.8.0"
  },
  "devDependencies": {
    "concurrently": "4.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navin-navi/crown-clothing-react.git"
  },
  "keywords": [
    "e-commerce",
    "react",
    "node",
    "hooks",
    "redux",
    "saga",
    "thunk"
  ],
  "author": "Navin",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/navin-navi/crown-clothing-react/issues"
  },
  "homepage": "https://github.com/navin-navi/crown-clothing-react#readme"
}
